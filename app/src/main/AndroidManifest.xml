        <activity android:name=".PaymenDetails" />
        <activity android:name=".EditPaymentDetails" />
        <activity android:name=".EditProfileStudent" />
        <activity android:name=".EditSegments" />
        <activity android:name=".SegmentsAndSubjects" />
        <activity android:name=".SignupSegment" />
        <activity android:name=".SignupSubjects" />
        <activity android:name=".Aboutandarea" />
        <activity android:name=".ImageUpload" />
        <activity android:name=".Editprofiletutor" />
        <activity android:name=".Information" />
        <activity android:name=".Paypage" />
        <activity android:name=".Managerprofile" />
        <activity android:name=".SearchTutorResult" />
    </application>

</manifest>